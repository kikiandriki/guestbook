{
  "name": "guestbook",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "settings": {},
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "skyapps.fish-vscode",
    "csstools.postcss",
    "ms-azuretools.vscode-docker"
  ],
  "forwardPorts": [3003, 6379, 8000, 80],
  "remoteUser": "node",
  "features": {
    "git": "latest",
    "github-cli": "latest",
    "aws-cli": "latest"
  }
}
